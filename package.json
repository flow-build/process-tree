{
  "name": "@flowbuild/process-tree",
  "version": "1.0.3",
  "description": "solution to store process hierarchies",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint ./src --ext .js --max-warnings=0",
    "lint:fix": "eslint ./src --ext .js --max-warnings=0 --fix",
    "build": "tsc",
    "migrations": "knex --env ${KNEX_ENV} --knexfile knexfile.js migrate:latest",
    "seeds": "knex --env ${KNEX_ENV} --knexfile knexfile.js seed:run",
    "migrations:local": "knex --env test --knexfile knexfile.js migrate:latest",
    "seeds:local": "knex --env test --knexfile knexfile.js seed:run",
    "get-version": "echo $npm_package_version",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flow-build/process-tree.git"
  },
  "keywords": [
    "flowbuild"
  ],
  "author": "FDTE-DSD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flow-build/process-tree/issues"
  },
  "homepage": "https://github.com/flow-build/process-tree#readme",
  "dependencies": {
    "dotenv": "16.0.3",
    "knex": "2.4.2",
    "lodash": "4.17.21",
    "pg": "8.9.0",
    "uuid": "9.0.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.191",
    "@types/uuid": "9.0.1",
    "eslint": "8.35.0",
    "jest": "29.4.3",
    "nodemon": "2.0.21",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  }
}
